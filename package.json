{
  "name": "bmdb",
  "version": "1.0.0",
  "description": "react version of TMDB",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "deploy": "gh-pages-deploy",
    "clean-source": "rimraf README.md app",
    "clean-old": "rimraf styles svgs scripts build index.html"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "clean-old",
      "build"
    ],
    "post": [
      "clean-source"
    ],
    "noprompt": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prashjain309/bmdb.git"
  },
  "keywords": [
    "bmdb",
    "react"
  ],
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "author": "Prashanth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prashjain309/bmdb/issues"
  },
  "homepage": "https://github.com/prashjain309/bmdb/",
  "dependencies": {
    "eslint": "^4.19.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.0.13"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "gh-pages-deploy": "^0.4.2",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.1.0",
    "install": "^0.11.0",
    "npm": "^5.8.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.3.0",
    "webpack-dev-server": "^3.1.1"
  }
}
